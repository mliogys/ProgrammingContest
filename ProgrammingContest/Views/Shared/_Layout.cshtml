<!DOCTYPE html>

<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="icon" href="~/favicon.ico">
    <title>@ViewBag.Title</title>
    <link href="~/Content/bootstrap.min.css" rel="stylesheet">
    <link href="~/Content/ie10-viewport-bug-workaround.css" rel="stylesheet">
    <link href="~/Content/signin.css" rel="stylesheet">
    <link href="~/Content/MyCSS.css" rel="stylesheet" />
    <script src="~/Scripts/ie-emulation-modes-warning.js"></script>
    <script src="~/Scripts/jquery-1.10.2.min.js"></script>  
</head>

<body>

    <div class="container color-scheme3">
        <div class="row">
            <div class="col-lg-6 col-sm-6 text-left" style="height: 20px;"><span id="time"></span></div>
            <div class="col-lg-6 col-sm-6 text-right"><span id="points"></span></div>
        </div>
    </div>

    <div class="text-center container color-scheme1">
        <h3>Programavimo konkursas</h3>
        <h4>Geriausia programuotojų komanda</h4>
    </div>
    <div class="text-center container color-scheme3" style="height: 10px;" >
    </div>
    <div class="container">

        @RenderBody()

    </div>

    @*<div class="text-center color-scheme1" id="footer">
        <p>&copy; Programinės įrangos katedra, @DateTime.Now.Year </p>
    </div>*@

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
    <script src="~/Scripts/ie10-viewport-bug-workaround.js"></script>
    
</body>
</html>

